<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="wrap_content" >
	<TextView android:id="@+id/type" android:layout_width="match_parent"
		android:layout_height="wrap_content" style="@style/NewsList" />
	<TextView android:id="@+id/start" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_below="@id/type"/>
	<TextView android:id="@+id/stop" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_toRightOf="@id/start"
		android:layout_alignBottom="@id/start" />
	<TextView android:id="@+id/note" android:layout_width="match_parent"
		android:layout_height="wrap_content" android:layout_below="@id/start"/>
</RelativeLayout>