<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="match_parent" style="@style/NewsList">
	<TextView android:id="@+id/type" android:layout_width="match_parent"
		android:layout_height="wrap_content" />
	<TextView android:id="@+id/start" android:layout_width="match_parent"
		android:layout_height="wrap_content" />
	<TextView android:id="@+id/stop" android:layout_width="match_parent"
		android:layout_height="wrap_content" />
	<TextView android:id="@+id/note" android:layout_width="match_parent"
		android:layout_height="wrap_content" />
</LinearLayout>